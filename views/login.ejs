<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Video call</title>
    <link rel="stylesheet" href="bootstrap/css/bootstrap.css">
    <link rel="stylesheet" href="bootstrap/css/bootstrap-theme.css">
  </head>
  <body>
    <div class="row">
      <div class="col-md-4 col-md-offset-4" style="margin-top:50px">
        <div class="panel panel-primary">
          <div class="panel-heading text-center">
              <h3><b>Join</b></h3>
          </div>
          <div class="panel-body">
              <div class="form-horizontal">
                <div class="form-group">
                  <label for="username" class="col-sm-2 col-md-2 control-label">User name</label>
                  <div class="col-md-7 col-sm-7">
                    <input type="text" class="form-control" id="username" placeholder="Type your name">
                  </div>
                  <div class="col-sm-2 col-md-2">
                    <button type="button" id="register" class="btn btn-success">Register</button>
                  </div>
                </div>

            </div>
          </div>
        </div>
      </div>
    </div>

  </body>
  <script src="socket.io.js"></script>
  <script src="jquery.min.js"></script>
  <script src="login.js"></script>
</html>
